{
  "name": "yam_api",
  "version": "1.0.0",
  "description": "An API with NestJS",
  "author": "MM Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --forceExit",
    "typeorm": "typeorm-ts-node-esm",
    "typeorm:datasource": "typeorm-ts-node-esm -d ./src/utils/typeorm-config.ts",
    "migration:generate": "npm run typeorm:datasource -- migration:generate ./src/database/migrations/$npm_config_name",
    "migration:create": "npm run typeorm -- migration:create ./src/database/migrations/$npm_config_name",
    "migration:run": "npm run typeorm:datasource -- migration:run",
    "migration:revert": "npm run typeorm:datasource -- migration:revert",
    "deploy": "npm ci && npm run build && npm run start:prod",
    "ts-node": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js -d ./src/utils/typeorm-config.ts",
    "seed:root": "npm run ts-node -- seed -s RootSiteSeeder",
    "seed:dev": "npm run ts-node -- seed"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.8",
    "@nestjs/common": "^8.4.5",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.4.7",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.5",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@ntegral/nestjs-sentry": "^3.0.7",
    "@sentry/browser": "^7.15.0",
    "@sentry/node": "^6.19.7",
    "@sentry/tracing": "^7.15.0",
    "aws-sdk": "^2.1237.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "better-sqlite3": "^7.1.2",
    "bunyan": "^1.8.15",
    "bunyan-pretty-colors": "^0.1.7",
    "cache-manager": "^3.6.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cli-table": "^0.3.11",
    "docx": "^8.0.1",
    "dotenv": "^16.0.2",
    "dotenv-expand": "^8.0.3",
    "express-http-context": "^1.2.4",
    "express-jwt": "^7.7.5",
    "ffmpeg-static": "^5.1.0",
    "helmet": "^5.1.0",
    "js-sha256": "^0.9.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.4",
    "lodash": "^4.17.21",
    "merge-img": "^2.1.3",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.35",
    "mysql2": "^2.3.3",
    "on-headers": "^1.0.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sharp": "^0.31.3",
    "swagger-ui-express": "^4.4.0",
    "typeorm": "^0.3.10",
    "typeorm-extension": "^2.1.15",
    "uuid-int": "^3.1.0",
    "webpack": "^5.72.1",
    "word-extractor": "^1.0.4",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@faker-js/faker": "^7.4.0",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^4.0.0",
    "@types/cli-table": "^0.3.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.0.0",
    "@types/on-headers": "^1.0.0",
    "@types/source-map-support": "^0.5.4",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*Table.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testTimeout": 60000
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "testFile.{ts,js}": [
      "npm run lint -- --cache --fix"
    ],
    "testFile.js": "eslint --cache --fix"
  }
}
